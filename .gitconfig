[user]
  name = ebith
  email = ebith.h@gmail.com
[color]
  ui = true
[diff]
  # external = ~/dotfiles/git_diff_wrapper
[pager]
  # diff =
[alias]
  br = branch
  ci = commit -v
  co = checkout
  ls = log --stat
  sm = submodule
  st = status
  up = pull --rebase
  amend = commit --amend -C HEAD
[core]
  whitespace = cr-at-eol
  editor = vim
  excludesfile = ~/.gitignore.global
  filemode = false
  autocrlf = false
  safecrlf = false
  quotepath = false
[i18n]
  commitencoding = UTF-8
[push]
  default = simple
