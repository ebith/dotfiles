[user]
  name = ebith
  email = ebith@feelmy.net
[color]
  ui = true
[diff]
  # external = ~/dotfiles/git_diff_wrapper
[pager]
  # diff =
[alias]
  br = branch
  ci = commit -v
  co = checkout
  ls = log --stat
  sm = submodule
  st = status
  up = pull --rebase
[core]
  excludesfile = ~/.gitignore.global
  filemode = false
  autocrlf = false
  safecrlf = false
  quotepath = false
[i18n]
  commitencoding = UTF-8
