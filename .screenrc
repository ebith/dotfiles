autodetach on
bell_msg "^G"
defkanji utf-8
defencoding utf-8
encoding utf-8 utf-8
defscrollback 10000
escape ^Vv
startup_message off
vbell off
caption always "%{= wg} %-w%{=bu dr}%n %t%{-}%+w %= %{=b wb}%m/%0d %{=b wm}%0C %A"

bind t screen
bind T title
bind h prev
bind l next
bind w windowlist
bind s

logfile "/home/ebith/.screen/%Y%m%d-%n.log"
log on
deflog on

defbce on
altscreen on

